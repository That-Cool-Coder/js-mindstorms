;" line2="        if (noteSections.length == 3) {&#13;" line3="            let noteName = noteSections[0];&#13;" line4="            let noteOctave = parseInt(noteSections[1]);&#13;" line5="            let noteBeats = parseFloat(noteSections[2]);&#13;" line6="            let pitch = noteToPitch[noteName];&#13;" line7="            pitch *= 2 ** (noteOctave - 3);&#13;" line8="&#13;" line9="            music.playTone(pitch, noteBeats);&#13;" line10="&#13;" line11="            // Music is async, so play wait for duration&#13;" line12="            let beatsPerSecond = music.tempo() / 60;&#13;" line13="            pause(noteBeats * beatsPerSecond * 1000);&#13;" line14="        }&#13;" line15="    });" numlines="16"></mutation></block></next></block></next></block></next></block></statement></block><block type="function_definition" id="%C$/ZCe;3,/W}=oHeohL" x="0" y="16"><mutation name="p_square" functionid="xUa7S+vT)c63O!5ch0}a"/><field name="function_name">p_square</field><statement name="STACK"><block type="variables_set" id=".^Ys3m!][^l+hYFTU6/8"><field name="VAR" id="%!J;LX1(w:@!S(5E)=RO">squareCount</field><value name="VALUE"><shadow type="math_number" id="k8.*`((1^yMhg0Cwwk(n"><field name="NUM">1</field></shadow></value><next><block type="variables_set" id="Xo0oE%,B!^kkgCUoQQ+9"><field name="VAR" id="O}ay+t8#vJlSUeaKd]T:">squareSize</field><value name="VALUE"><shadow type="math_number" id="rpo[suD@C[%JrLO.*n(%"><field name="NUM">50</field></shadow></value><next><block type="controls_repeat_ext" id="(hn{F5sZ0J]=q3bqnN1("><value name="TIMES"><shadow type="math_whole_number"><field name="NUM">0</field></shadow><block type="math_arithmetic" id="PDHb63[b1yX_kxhY9+S5"><field name="OP">MULTIPLY</field><value name="A"><shadow type="math_number"><field name="NUM">0</field></shadow><block type="variables_get" id="?eU|SP15435Vd7i2::#m"><field name="VAR" id="%!J;LX1(w:@!S(5E)=RO">squareCount</field></block></value><value name="B"><shadow type="math_number" id="N**3L[AlG5rEJeowWyj